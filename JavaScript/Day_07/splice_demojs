let arr = [10, 20, 30, 40, 50];
arr.splice(2, 1);
console.log(arr); // 30

arr.splice(2, 0, 30);
console.log(arr);

arr.splice(1, 2, 35, 45);
console.log(arr); // 30, 35, 45

let newArr = arr.toSpliced(1, 3);
console.log(arr, newArr); // 35, 45, 50

arr.push(60, 70, 80);
console.log(arr); // 35, 45, 50, 60, 70, 80

arr.pop();
console.log(arr); // 35, 45, 50, 60, 70

arr.shift();
console.log(arr); // 45, 50, 60, 70

arr.unshift(90);
console.log(arr); // 45, 50, 60, 70